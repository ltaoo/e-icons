  "bin": {
    "ei": "./bin/index.js"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "bin",
    "templates"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "prebuild": "node scripts/clean.js",
