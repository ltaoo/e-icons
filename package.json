    "@types/svgo": "^1.3.1",
    "@types/through2": "^2.0.34",
    "@types/vinyl-fs": "^2.4.11",
    "@umijs/fabric": "^2.2.2",
    "cross-env": "^7.0.0",
    "ts-node": "^8.6.2",
